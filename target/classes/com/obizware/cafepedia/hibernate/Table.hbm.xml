<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 <class catalog="cafepedia" name="com.obizware.cafepedia.model.Table" table="new_table">
  <id name="tableId" type="java.lang.Integer" >
   <column name="TableId"/>
   <generator class="identity"/>
  </id>
  <property generated="never" lazy="false" name="tableName" type="string">
   <column length="45" name="TableName" not-null="true"/>
  </property>
  <property generated="never" lazy="false" name="tableStatus" type="string">
   <column length="45" name="TableStatus" not-null="true"/>
  </property>
  <!-- <property generated="never" lazy="false" name="tableDesc" type="string">
   <column length="45" name="table_desc" not-null="true"/>
  </property> -->
 </class>
</hibernate-mapping>
